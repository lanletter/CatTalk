page{
    height: 100%;
    background-color:#feecf5;
}
.container{
    display: flex;
    flex-direction:column;
    align-items: center;
}
 swiper{
     position: absolute;
     top:0;
     width:670rpx;
     height:400rpx;
     margin:0 auto;
    border: 1px solid #ffffff;
    border-radius: 15rpx;
 }
 swiper image{
     width:100%;
     height:400rpx;
 }

.cats-col{
    position: absolute;
    top:426rpx;
    display: flex;
    flex-direction: column;
    width: 100%;
    height: 780rpx;
    overflow-y: auto;
}
.img-row{
    display: flex;
    flex-direction: row;
    flex-wrap:wrap;
    width: 100%;
    padding-left: 20rpx;
}
#img-cell{
    margin: 20rpx;
    width: 313rpx;
    height: 150rpx;
    border-radius: 15rpx;
}
.img-cell1{
    background-color: #ffffff;
    border: 1px solid #ffdcf0;
    color: #999999;
}
.img-cell2{
    background-color: #888888;
    border: 1px solid #888888;
    color: #ffffff;
}
#img-cell image{
    width: 100rpx;
    height: 100rpx;
    margin: 25rpx;
    vertical-align: middle;
}
#img-cell text{
    vertical-align: middle;
    font-size: 34rpx;
    width: 163rpx;
}
