page {
  height: 100%;
  width: 100%;
  background: #feecf5;
}

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.welcomeimg {
  width: 300rpx;
  height: 300rpx;
  border-radius: 500rpx;
  background: #fff;
}

.welcometxt {
  margin-top: 0rpx;
  font-size: 40rpx;
  font-weight: bold;
  color: #000000;
  letter-spacing: 15rpx;
  text-align: center;
}

.moto-container {
  margin-top: 0rpx;
  border: 2px solid #eda6ac;
  background: #eda6ac;
  width: 200rpx;
  height: 80rpx;
  text-align: center;
  border-radius: 5px;
}

.moto {
  font-size: 28rpx;
  line-height: 80rpx;
  color: #fff;
}

.shartxt {
    margin-top: 30rpx;
  font-size: 24rpx;
  color: #aaaaaa;
  display: block;
  letter-spacing: 0rpx;
